<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:Docs.View"
                    xmlns:vm="clr-namespace:Docs.ViewModel.TabWorkspace;assembly=ViewModel">
    
    <DataTemplate DataType="{x:Type vm:DocumentWorkspace}">
        <v:DocumentWorkspaceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AdministrationWorkspace}">
        <v:AdministrationWorkspaceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DirectoryWorkspace}">
        <v:DirectoryWorkspaceView />
    </DataTemplate>
    
</ResourceDictionary>