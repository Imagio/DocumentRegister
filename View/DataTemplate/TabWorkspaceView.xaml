<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:tab_workspace_viewv="clr-namespace:Docs.View.TabWorkspace"
                    xmlns:vm="clr-namespace:Docs.ViewModel.TabWorkspace;assembly=ViewModel">
    
    <DataTemplate DataType="{x:Type vm:DocumentWorkspace}">
        <tab_workspace_viewv:DocumentWorkspaceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AdministrationWorkspace}">
        <tab_workspace_viewv:AdministrationWorkspaceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DirectoryWorkspace}">
        <tab_workspace_viewv:DirectoryWorkspaceView />
    </DataTemplate>
    
</ResourceDictionary>