<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:Docs.View"
                    xmlns:vm="clr-namespace:Docs.ViewModel.Workspace;assembly=ViewModel">
                    
    
    <DataTemplate DataType="{x:Type vm:DecreeWorkspace}">
        <v:DecreeWorkspaceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AccountWorkspace}">
        <v:AccountWorkspaceView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:DepartmentWorkspace}">
        <v:DepartmentWorkspaceView />
    </DataTemplate>

</ResourceDictionary>