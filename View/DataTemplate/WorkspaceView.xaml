<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:workspace_view="clr-namespace:Docs.View.Workspace"
                    xmlns:vm="clr-namespace:Docs.ViewModel.Workspace;assembly=ViewModel">
                    
    
    <DataTemplate DataType="{x:Type vm:DecreeWorkspace}">
        <workspace_view:DecreeWorkspaceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AccountWorkspace}">
        <workspace_view:AccountWorkspaceView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:DepartmentWorkspace}">
        <workspace_view:DepartmentWorkspaceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:EmployeeWorkspace}">
        <workspace_view:EmployeeWorkspaceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SendingGroupWorkspace}">
        <workspace_view:DirectoryWorkspaceView />
    </DataTemplate>        
    

</ResourceDictionary>