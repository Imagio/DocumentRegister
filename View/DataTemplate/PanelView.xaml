<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:panel_view="clr-namespace:Docs.View.Panel"
                    xmlns:vm="clr-namespace:Docs.ViewModel.TabWorkspace;assembly=ViewModel">

    <DataTemplate DataType="{x:Type vm:DocumentWorkspace}">
        <panel_view:DocumentView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AdministrationWorkspace}" />
    <DataTemplate DataType="{x:Type vm:DirectoryWorkspace}" />

</ResourceDictionary>